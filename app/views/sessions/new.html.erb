<%= flash[:message] %>
<%= flash[:login_message] %>
<h2><%= form_for :sessions, url: login_path do |f| %></h2>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.password_field :password %>
  <%= f.submit "Login" %>
<% end %>
<p><%= link_to "Create New Account", new_user_path %></p>
