<h1>You must create account to view this website</h1>
<%= form_for :sessions, url: login_path, method: 'post' do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.password_field :password %>
  <%= f.submit "Login" %>
<% end %>
<p><%= link_to "Create New Account", new_user_path, data: { turbolinks: false } %></p>
